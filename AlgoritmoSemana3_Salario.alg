algoritmo "Algoritmo Semana 3"
// Autor : Gustavo Patan
// Data : 16/05/2022
// Seção de Declarações 
var
Salario: real
SalarioFinal: real

inicio
// Seção de Comandos 
   escreval("Informe seu salário (R$): ")
   leia (Salario)
   SalarioFinal <- Salario * 1.3)
   escreval ("Seu salário com adicional de 30% é (R$): ", SalarioFinal)
fimalgoritmo
